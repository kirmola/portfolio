{
  "builds": [
    {
      "src": "portfolio/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "buildCommand": "python3.9 manage.py tailwind build"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio/wsgi.py"
    }
  ]
}
